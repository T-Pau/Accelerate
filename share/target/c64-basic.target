.include "c64.inc"

.extension "prg"

.section zero_page {
    type: reserve_only
    address [
        : $02
        : $fb - $fe
    ]
}

.segment program {
    address: $080e - $7fff
}

.section code {
    segment: program
}

.section data {
    segment: program
}

.section reserved {
    type: reserve_only
    segment: program
}

.output {
    .data $0801 ; load address
    .data $080c, 2023, $9e ; BASIC: 2023 SYS
    .if start < 10000 {
      .data " "
    }
    .data start:string, 0 ; start
    .data $0000 ; end BASIC of program
    .memory $080e, .data_end
}
