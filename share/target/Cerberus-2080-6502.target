.cpu "65c02"

.extension "bin"

.section zero_page {
    type: reserve_only
    address: $02 - $ff
}

.segment program {
    address: $0205 - $efff
}

.section code {
    segment: program
}

.section data {
    segment: program
}

.section reserved {
    type: reserve_only
    segment: program
}

.output {
    .data $4c, start
    .memory $0205, data_end
}
