description Test instruction with 2 variants, known which will be used after assembly
arguments --create-library --target 6502 file.s
file file.s <inline>
.section code
.public test {
    sta ptr
}
.section zero_page
.public ptr .reserve 2
end-of-inline-data

file file.lib {} <inline>
.format_version 1.0
.target "6502"
.object ptr {
    visibility: public
    section: zero_page
    reserve: 2
}
.object test {
    visibility: public
    section: code
    body <
        .data $85, ptr:1
    >
}
end-of-inline-data
