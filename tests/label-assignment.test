description Test function expansion
arguments --create-library --target 6502 a.s

file a.s <inline>
.section code
test {
    lda foo
.private next = label + 1
label:
    nop
}
.section zero_page
foo .reserve 1
end-of-inline-data

file a.lib {} <inline>
.format_version 1.0
.target "6502"
.constant next {
    visibility: private
    value: (test+$03)
}
.object foo {
    visibility: private
    section: zero_page
    reserve: 1
}
.object test {
    visibility: private
    section: code
    body <
        .data $a5, foo:1, $ea
    >
}
end-of-inline-data
