arguments --create-library -o l.lib a.o b.o
file a.o <inline>
.format_version 1.0
.target "6502"
.constant exported_label {
    visibility: private
    value: (test+.label_offset(test, label))
}
.object test {
    visibility: private
    section: code
    body <
        .if .in_range($00, $ff, tmp1)
          .data $85, tmp1:1
        .else
          .data $8d, tmp1:2
        .end
      label:
        .data $91, ptr1:1, $60
    >
}
end-of-inline-data
file b.o <inline>
.format_version 1.0
.target "6502"
.object tmp1 {
    visibility: private
    section: zero_page
    reserve: 1
}
end-of-inline-data
file l.lib {} <inline>
.format_version 1.0
.target "6502"
.constant exported_label {
    visibility: private
    value: (test+$02)
}
.object test {
    visibility: private
    section: code
    body <
        .data $85, tmp1:1, $91, ptr1:1, $60
    >
}
.object tmp1 {
    visibility: private
    section: zero_page
    reserve: 1
}
end-of-inline-data
