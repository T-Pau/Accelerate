description Test function expansion
arguments -c --target 6502 a.s
file a.s <inline>
.section code
test {
    nop
    bne label
    nop
.local label:
.local next = label + 1
    nop
    bne label
    nop
}
end-of-inline-data
file a.o {} <inline>
.format_version 1.0
.target "6502"
.constant label {
    visibility: local
    value: (test+$04)
}
.constant next {
    visibility: local
    value: (test+$05)
}
.object test {
    visibility: local
    section: code
    body <
        .data $ea, $d0, $01:-1, $ea, $ea, $d0, -$03, $ea
    >
}
end-of-inline-data
