description Test instruction with 2 variants, unknown which will be used
arguments -c --target 6502 file.s
file file.s <inline>
.section code
.global test {
    sta ptr
}
end-of-inline-data
file file.o {} <inline>
.format_version 1.0
.target "6502"
.object test {
    visibility: global
    section: code
    body <
        .if .in_range($00, $ff, ptr)
          .data $85, ptr:1
        .else
          .data $8d, ptr:2
        .end
    >
}
end-of-inline-data
